modules {
  module {
    name = "mysql"
    language = "python"
    param RefreshRate {
        value = 2
    }
    param Host {
        value = 192.168.10.1
    }
    param Port {
	value = 3306
    }
    param User {
 	value = xxxx	
    }
    param Password {
 	value = xxxxx
    }
  }
}

collection_group {
  collect_every = 10
  time_threshold = 20
  name_match = "mysql_(.*)"
  name = "mysql_\\1"
  value_threshold = 1.0
}
