modules {
  module {
    name = "mysql_3306"
    language = "python"
    param RefreshRate {
        value = 2
    }
    param Host {
        value = "192.168.0.200"
    }
    param Port {
	value = "3306"	
    }
    param User {
 	value = "xxxx"	
    }
    param Password {
 	value = "xxxx"	
    }
    param Mysql {
	value = "/usr/local/mysql/bin/mysql"	
    }
  }
}

collection_group {
  collect_every = 10
  time_threshold = 20
  metric {
    name_match = "mysql_(.*)"
    name = "mysql_\\1"
    value_threshold = 1.0
  }
}
